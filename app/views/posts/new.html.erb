<h1>New Post </h1>

<div class='container'>
  <%= form_for @post do |f| %>

  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>

  <div>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>

  <div>
    <%= f.label :type %>
    <%= f.text_field :type %>
  </div>

  <%= f.submit %>

<%end%>

</div>
